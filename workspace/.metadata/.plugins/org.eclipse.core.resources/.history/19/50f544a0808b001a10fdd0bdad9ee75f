
public class ForMission8 {

	public static void main(String[] args) {
		int star = 5;
		int i;
		int j;
		
		
		
		
		for(i = star; i >= 1; i--) {
			for(j = 1; j<i; j++ ) {
				System.out.print(" ");
			}
			for(j = 1; j<=i; j++) {
			System.out.print("*");
			}
			System.out.println();
		}
		


	}

}
