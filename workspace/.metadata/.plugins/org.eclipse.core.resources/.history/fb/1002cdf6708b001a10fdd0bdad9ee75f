
public class ForMission6 {

	public static void main(String[] args) {
		int star = 6;
		int i;
		int j;
		
//		for(i = 0; i < star; i++) {
//			for(j = 0; j < star; j++) {
//				System.out.printf("*");
//			}
//			System.out.println();
//		}
		
		for(i = 0, j = 0; i < star ; ) {
			
			if(j < star) {
			System.out.printf("*");
			j++;
			System.out.println();
			} else {
				System.out.printf("*");
				i++;
				j = 0;
//				System.out.println();
				
			}
		}

	}

}
