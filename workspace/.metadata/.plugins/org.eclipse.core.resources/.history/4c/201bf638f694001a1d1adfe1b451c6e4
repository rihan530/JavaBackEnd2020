package com.koreaIT.first.level4;

public class Human {
	private String name;
	private int age;
	
	public Human() {
//		System.out.println("기본생성자 호출");
//		this.name = "이순신";
//		this.age = 50;
		this("이순신", 50);
	}

	public Human(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}

	public Human(String name) {
		
	}

	public void printInfo() {
		System.out.printf(" 이름 : %s\n 나이 : %d\n", this.name, this.age);

	}
}













