
public class ForMission5 {

	public static void main(String[] args) {
		
		int i = 2;		
		int j = 1;

		
		for(; i < 10; i++) {
			System.out.printf("----%dë‹¨----\n", i);
			System.out.printf("%d x %d = %d\t", i, j, i*j);			
			for(; j < 10; j++) {
				System.out.printf("%d x %d = %d\t", i, j, i*j);
			}
			System.out.println();
//			System.out.println("----------");
		}

	}

}
