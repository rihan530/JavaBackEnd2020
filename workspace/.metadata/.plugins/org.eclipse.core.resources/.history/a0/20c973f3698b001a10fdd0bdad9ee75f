
public class ForMission5 {

	public static void main(String[] args) {
		
		int j = 1;
		int i = 1;
		
		for(; j < 10; j++) {
			System.out.printf("----%dë‹¨----\n", i);
			System.out.printf("%d x %d = %d\n", i, j, i*j);			
			for(; i < 10; i++) {
				System.out.printf("%d x %d = %d\t", i, j, i*j);
			}
			
			System.out.println("----------");
		}

	}

}
