
public class ForMission6 {

	public static void main(String[] args) {
		int star = 6;
		
		for(int i = 0; i < star; i++) {
			for(int j = 0; j < star; j++) {
				System.out.printf("*");
			}
			System.out.println();
		}
		
		for(int i = 0, j = 0; i < star ;i++,j++) {
			System.out.printf("*");
			
		}

	}

}
