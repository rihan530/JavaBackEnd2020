import java.util.Scanner;

public class ConditionMission {

	public static void main(String[] args) {
		int season;
		
		System.out.print("월을 입력해 주세요 : (1~12) ");
		Scanner sc = new Scanner(System.in);
		season = sc.nextInt();
		
//		switch(season) {
