package com.koreaIT.first.level3;

public class Car {
	//멤버필드
	boolean powerOn;
	String color;
	int wheel;
	int speed;
	boolean wiperOn;
	
	//멤버 메소드
	void power() {
		powerOn = !powerOn;
	}
	
	void speedUp() {
		speed++;
	}
	
	void speedDown() {
		speed--;
	}
	
	void wiper() {
		wiperOn = !wiperOn;
	}
}
