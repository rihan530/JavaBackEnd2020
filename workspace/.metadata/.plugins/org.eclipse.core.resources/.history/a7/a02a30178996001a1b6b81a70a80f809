package com.koreaIT.first.level5;

public class Calc {
	static int csum;
	static int rsum;
	static int avg;
	
	public static void printSumAvg(int[][] score) {
		for(int j = 0; j < score.length; j++) {
			for(int i = 0; i < score.length; i++) {
				csum += score[i][j];
				rsum += score[j][i];
			}
			System.out.println(csum + rsum);
			csum = 0;
			rsum = 0;
		}
	}
}
