
public class ForMission5 {

	public static void main(String[] args) {
		


		
		for(int i = 1 ; i < 10; i++) {
//			System.out.printf("----%dë‹¨----\n", i);
//			System.out.printf("%d x %d = %d\t", i, j, i*j);			
			for(int j = 2; j < 10; j++) {
				System.out.printf("%d x %d = %d\t", j, i, i*j);
			}
			System.out.println();
//			System.out.println("----------");
//			System.out.printf("%d x %d = %d\t", i, j, i*j);
		}

	}

}
